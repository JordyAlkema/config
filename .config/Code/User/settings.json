        "**/__pycache__/": true,
        "**/*.o": true,
        "**/*.class": true,
    },
    "files.exclude": {
        "**/.git/": true,
        "**/node_modules/": true,
        "**/__pycache__/": true,
        "**/*.o": true,
        "**/*.class": true,
    },

