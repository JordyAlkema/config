#!/usr/bin/env python

import math
from math import factorial
import sys


def C(n, k):
    return factorial(n)//(factorial(n-k)*factorial(k))


def P(n, k):
    return factorial(n)//(factorial(n-k))


if __name__ == '__main__':
    globals = vars(math)
    print(eval(sys.stdin.readline(), globals, locals()))
