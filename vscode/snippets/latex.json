{
	"base": {
		"prefix": "base",
		"body": [
			"\\documentclass[12pt]{article}",
			"",
			"\\usepackage[margin=1in]{geometry}",
			"\\usepackage{mathtools}",
			"\\usepackage{amssymb}",
			"\\usepackage{amsthm}",
			"\\usepackage{mathrsfs}",
			"",
			"\\DeclarePairedDelimiter{\\ceil}{\\lceil}{\\rceil}",
			"\\DeclarePairedDelimiter{\\floor}{\\lfloor}{\\rfloor}",
			"\\renewcommand\\qedsymbol{$\\blacksquare$}",
			"\\newtheorem{theorem}{Theorem}[section]",
			"\\newtheorem{corollary}{Corollary}[theorem]",
			"\\newtheorem{lemma}[theorem]{Lemma}",
			"\\theoremstyle{definition}",
			"\\newtheorem{definition}{Definition}[section]",
			"",
			"\\title{$1}",
			"\\date{$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE}",
			"\\author{Caleb Bassi}",
			"",
			"\\begin{document}",
			"",
			"\\maketitle",
			"",
			"$0",
			"",
			"\\end{document}"
		]
	},
	"images": {
		"prefix": "images",
		"body": [
			"\\usepackage{graphicx}  % images",
			"",
			"\\graphicspath{ {./images/} }"
		]
	},
	"venn": {
		"prefix": "venn",
		"body": [
			"\\usepackage{venndiagram}"
		]
	},
	"code": {
		"prefix": "code",
		"body": [
			"\\usepackage{listings}",
			"",
			"\\lstdefinestyle{mystyle}{",
			"	numbers=left,",
			"}",
			"\\lstset{style=mystyle}"
		]
	},
	"piecewise": {
		"prefix": "piecewise",
		"body": [
			"\\[",
			"	$1=",
			"	\\begin{cases}",
			"		$2 & $3 \\\\\\",
			"		$4 & $5 ",
			"	\\end{cases}",
			"\\]"
		]
	},
	"align": {
		"prefix": "align",
		"body": [
			"\\begin{align*}",
			"	$1",
			"\\end{align*}"
		]
	},
	"table": {
		"prefix": "table",
		"body": [
			"\\begin{center}",
			"	\\begin{tabular}{|c|c|}",
			"		\\hline",
			"		$1 & $2 \\\\\\",
			"		\\hline",
			"		$3 & $4 \\\\\\",
			"		\\hline",
			"	\\end{tabular}",
			"\\end{center}"
		]
	},
	"imptree": {
		"prefix": "imptree",
		"body": [
			"\\usepackage[linguistics]{forest}"
		]
	},
	"tree": {
		"prefix": "tree",
		"body": [
			"\\begin{center}",
			"	\\begin{forest}",
			"		[$0",
			"		]",
			"	\\end{forest}",
			"\\end{center}"
		]
	}
}
